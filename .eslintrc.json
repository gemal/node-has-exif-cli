{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "it": true,
        "describe": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "max-len": ["error", {"code": 120}],
      "indent": ["error", 4]
    }
}
